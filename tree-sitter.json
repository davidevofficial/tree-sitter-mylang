{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "mylang",
      "camelcase": "Mylang",
      "title": "Mylang",
      "scope": "source.mylang",
      "file-types": [
        "mylang"
      ],
      "injection-regex": "^mylang$",
      "class-name": "TreeSitterMylang"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "none",
    "description": "Mylang grammar for tree-sitter",
    "authors": [
      {
        "name": "davidevofficial",
        "email": "davidevufficial@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/davidevofficial/tree-sitter-mylang"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}